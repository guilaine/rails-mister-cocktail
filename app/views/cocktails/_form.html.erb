<%= simple_form_for(@cocktail) do |f| %>
 <% if @cocktail.errors.any? %>
   <div id="error_explanation">
     <ul>
     <% @cocktail.errors.full_messages.each do |message| %>
       <li><%= message %></li>
     <% end %>
     </ul>
   </div>
 <% end %>

<%= f.input :name %>
<%= f.input :photo %>
<%= f.input :photo_cache, as: :hidden %>

 <%= f.button :submit %>
 <%= link_to "Back", cocktails_path, {class: "btn btn-primary"} %>
<% end %>
